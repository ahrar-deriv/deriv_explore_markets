# Auto detect text files and perform LF normalization
* text=auto

# Dart files
*.dart text

# Documentation
*.md text
*.txt text

# Scripts
*.sh text eol=lf

# Binary files
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.mov binary
*.mp4 binary
*.mp3 binary
*.flv binary
*.fla binary
*.swf binary
*.gz binary
*.zip binary
*.7z binary
*.ttf binary
*.eot binary
*.woff binary
*.pyc binary
*.pdf binary
*.ez binary
*.bz2 binary
*.swp binary

# Flutter/Dart specific
pubspec.lock linguist-generated=true
*.g.dart linguist-generated=true
*.freezed.dart linguist-generated=true
*.mocks.dart linguist-generated=true

